{
  "name": "nw-work-space",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "cd projects/weather-components-ui && ng build --configuration production",
    "build:component": "ng build web-component --configuration production --output-hashing none",
    "build:copy": "cp dist/web-component/browser/main.js projects/Vanilla-JS-app/main.js && cp dist/web-component/browser/polyfills.js projects/Vanilla-JS-app/polyfills.js",
    "watch": "ng build --watch --configuration development",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:lib": "jest --coverage --config ./jest.lib.config.ts",
    "test:app": "jest --coverage --config ./jest.app.config.ts",
    "package": "cat dist/web-component/browser/{main,polyfills}.js > dist/web-component/browser/component-lib.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/elements": "^17.2.3",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@angular/service-worker": "^17.2.0",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.2",
    "@angular/cli": "^17.2.1",
    "@angular/compiler-cli": "^17.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "ng-packagr": "^17.2.0",
    "typescript": "~5.3.2"
  }
}
